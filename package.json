{
  "name": "n8n-nodes-column",
  "version": "1.0.0",
  "description": "A comprehensive n8n community node for Column bank platform providing 23 resources and 150+ operations for banking automation including entities, accounts, ACH/wire transfers, checks, cards, loans, and webhooks.",
  "license": "BUSL-1.1",
  "author": {
    "name": "Velocity BPA",
    "email": "licensing@velobpa.com",
    "url": "https://velobpa.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Velocity-BPA/n8n-nodes-column.git"
  },
  "homepage": "https://github.com/Velocity-BPA/n8n-nodes-column#readme",
  "bugs": {
    "url": "https://github.com/Velocity-BPA/n8n-nodes-column/issues"
  },
  "keywords": [
    "n8n",
    "n8n-community-node-package",
    "column",
    "banking",
    "fintech",
    "ach",
    "wire-transfer",
    "bank-account",
    "payments",
    "loan",
    "card-issuing",
    "check",
    "baas",
    "banking-as-a-service"
  ],
  "main": "dist/nodes/Column/Column.node.js",
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/ColumnApi.credentials.js",
      "dist/credentials/ColumnOAuth.credentials.js"
    ],
    "nodes": [
      "dist/nodes/Column/Column.node.js",
      "dist/nodes/Column/ColumnTrigger.node.js"
    ]
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc && gulp build:icons",
    "dev": "tsc --watch",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "format": "prettier --write \"**/*.ts\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "prepublishOnly": "npm run build",
    "clean": "rimraf dist"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.0",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.15.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-n8n-nodes-base": "^1.16.1",
    "gulp": "^4.0.2",
    "jest": "^29.7.0",
    "n8n-workflow": "*",
    "prettier": "^3.2.4",
    "rimraf": "^5.0.5",
    "ts-jest": "^29.1.2",
    "typescript": "^5.3.3"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  },
  "dependencies": {
    "axios": "^1.6.5",
    "crypto-js": "^4.2.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
